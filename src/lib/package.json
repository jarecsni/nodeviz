{
  "name": "nodeviz",
  "version": "0.2.0",
  "description": "A node graph based UI framework",
  "author": "Johnny Jarecsni <johnny.jarecsni@icloud.com>",
  "license": "BSD-3-Clause",
  "keywords": [
    "svelte",
    "sveltekit",
    "graph",
    "render",
    "framework"
  ],
  "devDependencies": {
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "@types/jest": "^28.1.1",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "prettier": "^2.6.2",
    "prettier-plugin-svelte": "^2.7.0",
    "svelte": "^3.48.0",
    "svelte-check": "^2.7.2",
    "svelte-jester": "^2.3.2",
    "svelte-preprocess": "^4.10.7",
    "svelte2tsx": "^0.5.10",
    "tslib": "^2.4.0",
    "ts-jest": "^28.0.4",
    "typescript": "^4.7.3",
    "uuid": "^8.3.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jarecsni/nodeviz"
  },
  "bugs": {
    "url": "https://github.com/jarecsni/nodeviz/issues"
  },
  "type": "module",
  "svelte": "index.js",
  "jest": {
    "transform": {
      "^.+\\.svelte$": [
        "svelte-jester",
        {
          "preprocess": true
        }
      ],
      "^.+\\.ts$": "ts-jest",
      "^.+\\.js$": "babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "ts",
      "svelte"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/package"
    ]
  },
  "exports": {
    "./package.json": "./package.json",
    "./Node.test": "./Node.test.js",
    "./Node": "./Node.js",
    "./components/treeview/TreeBranch.svelte": "./components/treeview/TreeBranch.svelte",
    "./components/treeview/TreeLeaf.svelte": "./components/treeview/TreeLeaf.svelte",
    "./components/treeview/TreeView.svelte": "./components/treeview/TreeView.svelte",
    "./components/treeview": "./components/treeview/index.js",
    ".": "./index.js"
  }
}