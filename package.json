{
  "name": "nodeviz",
  "version": "0.0.3",
  "description": "A node graph based UI framework",
  "author": "Johnny Jarecsni <johnny.jarecsni@icloud.com>",
  "license": "BSD-3-Clause",
  "keywords": [
    "svelte",
    "sveltekit",
    "graph",
    "render",
    "framework"
  ],
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "@types/jest": "^27.5.1",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "prettier": "^2.4.1",
    "prettier-plugin-svelte": "^2.4.0",
    "svelte": "^3.44.0",
    "svelte-check": "^2.2.6",
    "svelte-jester": "^2.3.2",
    "svelte-preprocess": "^4.9.4",
    "svelte2tsx": "^0.5.3",
    "tslib": "^2.3.1",
    "ts-jest": "^28.0.3",
    "typescript": "^4.4.3",
    "uuid": "^8.3.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jarecsni/nodeviz"
  },
  "bugs": {
    "url": "https://github.com/jarecsni/nodeviz/issues"
  },
  "type": "module",
  "svelte": "index.js",
  "jest": {
    "transform": {
      "^.+\\.svelte$": [
        "svelte-jester",
        {
          "preprocess": true
        }
      ],
      "^.+\\.ts$": "ts-jest",
      "^.+\\.js$": "babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "ts",
      "svelte"
    ],
    "modulePathIgnorePatterns": ["<rootDir>/package"]
  }
}
